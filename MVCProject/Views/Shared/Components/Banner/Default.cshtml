@model List<int>

@if(Model.Count>0)
{
    <table>
        <thead>
            <tr>
                <th>RollNo</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item</td>
                </tr>
            }
        </tbody>
    </table>
}
else{
    <h1>No record available</h1>
}